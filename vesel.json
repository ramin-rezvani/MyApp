{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "pythonVersion": "3.11",
          "includeFiles": [
            "templates/**",
            "static/**"
          ],
          "installCommand": "pip install -r requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "app.py"
      }
    ]
  }