{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "includeFiles": [
            "templates/**",
            "static/**",
            "controller/**",
            "models.py",
            "extensions.py"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }